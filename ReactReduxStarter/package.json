{
  "name": "ps-redux",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "@storybook/react": "^3.1.9",
    "axios": "^0.16.2",
    "babel-polyfill": "6.8.0",
    "bootstrap": "3.3.6",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.3",
    "material-ui": "^1.0.0-beta.2",
    "material-ui-icons": "^1.0.0-alpha.19",
    "nock": "^9.0.14",
    "react": "^15.4.0",
    "react-confirm-bootstrap": "^3.1.4",
    "react-confirm-dialog": "^1.0.5",
    "react-dom": "^15.4.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.1.9",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "cheerio": "^0.20.0",
    "colors": "1.1.2",
    "compression": "^1.7.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "react-test-renderer": "^15.6.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/pluralsight-redux-starter"
  },
  "jest": {
    "testMatch": [
      "**/axios.test.js"
    ]
  }
}
