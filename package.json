    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.0.1",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.12.0",
    "json-schema-faker": "0.4.1",
    "json-server": "0.9.6",
